        <PublishAot>false</PublishAot>
        <InvariantGlobalization>true</InvariantGlobalization>
        <RootNamespace>Il2CppWasmBindgen</RootNamespace>
        <TypeScriptCompileBlocked>true</TypeScriptCompileBlocked>
